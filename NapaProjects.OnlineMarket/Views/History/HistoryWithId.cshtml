@model IEnumerable<HistoryModel>

<div class="p-2 bg-info">
    <h3>
         History for Product which Name is @ViewBag.ProductName
    </h3>
</div>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <td>Category</td>
            <td>Price</td>
            <td>State</td>
            <td>Date</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.RelatedCategoryName</td>
                <td>@p.Price</td>
                <td>@p.State</td>
                <td>@p.Date</td>
            </tr>
        }
    </tbody>
</table>

<a href="@ViewBag.ReturnUrl" class="btn btn-primary">Cancel</a>